{
  "name": "taskmanagement",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist && mkdir dist",
    "lint": "esw  ./test ./routes ./models/ app.js bin/www ",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch",
    "test": "cross-env NODE_ENV=test mocha",
    "test:report": "nyc mocha",
    "compile": "babel ./ --out-dir dist/ --ignore ./node_modules,./test --copy-files ",
    "start:prod": "cross-env NODE_ENV=dev node dist/bin/www",
    "start:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "prebuild:dev": "npm-run-all test clean",
    "build": "npm run compile",
    "build:dev": "npm run compile",
    "postbuild:dev": "npm run start:prod",
    "build:release": "npm-run-all  test clean compile ",
    "postbuild:release": "rimraf dist/.git dist/test dist/nodemon.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "bcrypt-nodejs": "latest",
    "body-parser": "~1.15.2",
    "connect-flash": "~0.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.17.3",
    "express-session": "~1.14.1",
    "http-errors": "~1.6.3",
    "method-override": "~2.3.6",
    "mongo": "^0.1.0",
    "mongoose": "^5.7.7",
    "morgan": "~1.9.1",
    "passport": "~0.3.2",
    "passport-facebook": "~2.1.1",
    "passport-google-oauth": "~1.0.0",
    "passport-local": "~1.0.0",
    "passport-twitter": "~1.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-watch": "^6.0.1",
    "mocha": "^6.2.0",
    "mongodb-memory-server": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2"
  }
}
